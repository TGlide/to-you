:root {
	/* ---------- */
	/* Typography */
	/* ---------- */
	--ff-display: 'Montserrat', sans-serif;
	--ff-body: 'Inter', sans-serif;

	--fs-micro: 0.75rem;
	--fs-s: 0.875rem;
	--fs-m: 1rem;
	--fs-l: 1.125rem;
	--fs-xl: 1.5rem;
	--fs-2xl: 2rem;
	--fs-3xl: 2.5rem;
	--fs-4xl: 4rem;

	--fw-light: 300;
	--fw-regular: 400;
	--fw-medium: 500;
	--fw-semibold: 600;
	--fw-bold: 700;

	--line-height-micro: 1rem;
	--line-height-s: 1.25rem;
	--line-height-m: 1.375rem;
	--line-height-l: 1.5rem;
	--line-height-xl: 2rem;
	--line-height-2xl: 2.5rem;
	--line-height-3xl: 2.75rem;
	--line-height-4xl: 3.875rem;

	--letter-spacing-tighter: -0.015em;
	--letter-spacing-tight: -0.005em;
	--letter-spacing-none: 0;
	--letter-spacing-loose: 0.002em;

	/* ------ */
	/* Radius */
	/* ------ */
	--radii-xs: 0.25rem; /* 4px */
	--radii-s: 0.5rem; /* 8px */
	--radii-m: 0.75rem; /* 12px */
	--radii-l: 1rem; /* 16px */
	--radii-xl: 1.5rem; /* 24px */
	--radii-2xl: 2rem; /* 32px */
	--radii-round: 999rem;

	/* ------ */
	/* Spaces */
	/* ------ */
	--space-0: 0;
	--space-1: 0.0625rem;
