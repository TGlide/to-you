	/* ------- */
	/* Z-index */
	/* ------- */
	--z-index-0: 0;
	--z-index-1: 100;
	--z-index-2: 200;
	--z-index-3: 300;
	--z-index-4: 400;
	--z-index-5: 500;
	--z-index-max: 999;

	/* ---------- */
	/* Animations */
	/* ---------- */
	--transition-appearance: 200ms ease;
	--transition-motion: 300ms cubic-bezier(0.2, 1, 0.2, 1);
	--transition-smooth: 400ms cubic-bezier(0.9, 0, 0.05, 1);
	--transition-in-out: 300ms ease-in-out;

	/* ------- */
	/* Palette */
	/* ------- */
	--palette-black-hsl: 0, 0%, 0%;
	--palette-black: hsla(var(--palette-black-hsl), 1);
	--palette-white-hsl: 0, 0%, 100%;
	--palette-white: hsla(var(--palette-white-hsl), 1);

