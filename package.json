{
	"name": "to-you",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint . --max-warnings 0",
		"format": "prettier --plugin-search-dir . --write .",
		"aw:local": "node ./scripts/init-local.js",
		"aw:prod": "node ./scripts/init-prod.js",
		"aw:deploy": "appwrite deploy collection --all"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"dotenv": "^16.0.3",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-extend-rule": "^4.0.0",
		"postcss-import": "^15.0.0",
		"postcss-load-config": "^4.0.1",
		"postcss-nested": "^5.0.6",
		"postcss-preset-env": "^7.7.2",
		"postcss-scrollbar": "^0.5.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/inter": "^4.5.13",
		"@fontsource/montserrat": "^4.5.13",
		"appwrite": "^10.1.0",
		"feather-icons": "^4.29.0",
		"uuid": "^9.0.0"
	}
}
